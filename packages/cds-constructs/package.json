{
  "name": "@gcoguiec/cds-constructs",
  "author": "Guillaume 'Yugo' Coguiec <mail@gcoguiec.com> (https://github.com/gcoguiec/)",
  "license": "BSD-2-Clause",
  "type": "module",
  "packageManager": "pnpm@7.27.1",
  "description": "Simplify and streamline stack composition via reusable constructs",
  "keywords": [
    "cdktf",
    "cdk8s",
    "constructs"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "rimraf dist && pnpm rollup -c rollup.config.ts --configPlugin typescript",
    "build:watch": "pnpm build --watch",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "pnpm lint --fix",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@cdktf/provider-aws": "^12.0.5",
    "@gcoguiec/eslint-config-typescript": "^1.0.3",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@swc/core": "^1.3.36",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "cdktf": "^0.15.5",
    "constructs": "^10.1.255",
    "cspell": "^6.26.3",
    "esbuild": "^0.17.10",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.4.3",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "rollup": "^3.17.2",
    "rollup-plugin-dts": "^5.2.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "@cdktf/provider-aws": "^12.0.5",
    "cdktf": "^0.15.5",
    "constructs": "^10.1.255"
  },
  "engines": {
    "node": ">=19",
    "pnpm": ">=7"
  }
}
